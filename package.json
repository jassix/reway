{
  "name": "@reway/core",
  "npmName": "@reway/core",
  "private": false,
  "version": "0.2.1",
  "description": "Reway core logic package",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jassix/reway.git"
  },
  "author": "Nikita Pitunov <jassix@pm.me> (https://github.com/jassix)",
  "scripts": {
    "start:dev": "nodemon",
    "test": "jest"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "default": "./esm/index.js"
    }
  },
  "main": "src/index.ts",
  "unpkg": "dist/index.js",
  "module": "esm/index.js",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js",
        "*.d.ts.map",
        "*.d.ts",
        "**/*.js",
        "**/*.d.ts.map",
        "**/*.d.ts"
      ]
    }
  ],
  "dependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@types/node": "^20.1.0",
    "node": "^20.1.0",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "bugs": {
    "url": "https://github.com/jassix/reway/issues"
  },
  "homepage": "https://github.com/jassix/reway#readme",
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "jest": "^29.5.0",
    "nanostores": "^0.8.1"
  },
  "keywords": [
    "react",
    "javascript",
    "typescript",
    "reactive",
    "state-management",
    "reactjs",
    "pubsub",
    "effector",
    "zustand",
    "jotai"
  ]
}
